@using MinimalApi.Dtos
@model List<UsuarioResponse>
@{
    ViewData["Title"] = "GetAll";
}

<h1>@ViewData["Title"]</h1>

<div>
    <details open>
        <summary>Mostrar Usuario</summary>
    @foreach(var usuario in Model)
    {
        <p>@usuario.IdUsuario</p>
        <P>@usuario.Nombre</P>
        <p>@usuario.Correo</p>
        <p>@usuario.Telefono</p>
    }
    </details>
    <a asp-action="AltaForm">
        <button type="button">Crear Usuario</button>
    </a>
</div>